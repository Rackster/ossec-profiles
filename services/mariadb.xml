<agent_config profile="service-mariadb">
	<syscheck>
		<directories check_all="yes">/etc/my.cnf</directories>
        <directories check_all="yes">/etc/my.cnf.d</directories>
	</syscheck>

	<localfile>
        <log_format>syslog</log_format>
        <location>/var/log/mariadb/mariadb.log</location>
	</localfile>
</agent_config>
